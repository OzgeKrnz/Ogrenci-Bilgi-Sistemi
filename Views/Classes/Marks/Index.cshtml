@model List<FirstTerm>

<!DOCTYPE html>

<html>

<table class="table table-hover table-bordered ">
    <tbody>
    <thead>
    <th scope="col" >Dönem Dersleri</th>
    <th scope="col">Vize</th>
    <th scope="col">Final</th>
    <th scope="col">Bütünleme</th>
    </thead>
    @foreach (var obj in Model)
    {
        <tr>
            <td> @obj.Classname</td>
            <td> @obj.Vize</td>
            <td> @obj.FinalNt</td>
            <td></td>
        </tr>
     
    }
    </tbody>
</table>



</html>